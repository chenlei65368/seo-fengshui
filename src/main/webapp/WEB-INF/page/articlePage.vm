        </div> 
        <div id="Article" oncopy="copy();"> 
         <div class="AD-300">
          <script type="text/javascript">BAIDU_CLB_SLOT_ID = "1174724";</script> 
          <script type="text/javascript" src="http://cbjs.baidu.com/js/o.js"></script>
         </div> ${page.content} 
        </div>
       </div> 
       <div class="article_paging">
        <ul></ul>
       </div> 
       <div class="AD-680">
        <script type="text/javascript">BAIDU_CLB_SLOT_ID = "1174725";</script> 
        <script type="text/javascript" src="http://cbjs.baidu.com/js/o.js"></script>
       </div> 
	   <div id="hm_t_92072"></div>
       <div class="tag1" style="width:687px;">
	   <ul>
          #foreach($randomTag in $page.randomFirstLevelTags)
           <li><a href="${randomTag.resourcePath}" title="${randomTag.name}" target="_blank">${randomTag.name}</a></li>#end
       </ul>
       </div>
       <div class="AD-680">
        <script type="text/javascript">BAIDU_CLB_SLOT_ID = "1174726";</script> 
        <script type="text/javascript" src="http://cbjs.baidu.com/js/o.js"></script>
       </div> 
       <div class="article_by"> 
        <div class="bytitle">
          关联词： #foreach($baiduWord in $page.baiduWords) 
         <h3 class="jiekou" title="${baiduWord}">$baiduWord</h3>， #end 
        </div>
        <div id="byshare"></div>
       </div>
	   <div class="articlelike">
	   <div id="hm_t_92072"></div>
	   </div>
       <div class="AD-680">
        <script type="text/javascript">BAIDU_CLB_SLOT_ID = "1174728";</script> 
        <script type="text/javascript" src="http://cbjs.baidu.com/js/o.js"></script>
       </div> 
       <div id="pinglun">
        <div class="box"> 
         <ul class="left-item"> 
          <div class="pic-title">
           <h3 class="ico-07"><font style="color:#FF7E00;">${page.title}</font>的评论</h3>
          </div> #foreach($comment in $page.commentList) 
          <li>${comment.nickName}的看法： <p> ${comment.content}。</p>
           <div class="lb">
           </div> #end </li>
         </ul>
        </div>
       </div> 
       <div class="cl"></div> 
      </div>
     </div> 
     <div class="article-right"> 
      <div class="right-item"> 
       <div class="title">
        <font style="font-size:18px;color: #373;">本周推荐</font>
       </div> 
       <div class="box">
         #set($i = 1) #foreach($article in $page.recommendArtis) #if($i < 4) 
        <li><span class="a b">$i</span><a href="${article.resourcePath}" title="${article.name}" target="_blank">${article.name}</a></li> #else 
        <li><span class="a">$i</span><a href="${article.resourcePath}" title="${article.name}" target="_blank">${article.name}</a></li> #end #set($i = $i + 1) #end 
        <div class="AD-240"> 
         <script type="text/javascript">BAIDU_CLB_SLOT_ID = "1174729";</script> 
         <script type="text/javascript" src="http://cbjs.baidu.com/js/o.js"></script> 
        </div>
       </div>
      </div> 
      <div class="right-item"> 
       <div class="title">
        <font style="font-size:18px;color: #373;">热点阅读</font>
       </div> 
       <div class="box">
         #set($i = 1) #foreach($article in $page.hotestArtis) #if($i < 4) 
        <li><span class="a b">$i</span><a href="${article.resourcePath}" title="${article.name}" target="_blank">${article.name}</a></li> #else 
        <li><span class="a">$i</span><a href="${article.resourcePath}" title="${article.name}" target="_blank">${article.name}</a></li> #end #set($i = $i + 1) #end 
       </div>
      </div> 
      <div class="AD-240"> 
       <script type="text/javascript">BAIDU_CLB_SLOT_ID = "1174730";</script> 
       <script type="text/javascript" src="http://cbjs.baidu.com/js/o.js"></script> 
      </div> 
      <div id="right-box"> 
       <div id="right-ad" class="item1"> 
        <div class="right-item"> 
         <div class="title">
          <font style="font-size:18px;color: #373;">最新文章</font>
         </div> 
         <div class="box">
           #set($i = 1) #foreach($article in $page.randomArtis) #if($i < 4) 
          <li><span class="a b">$i</span><a href="${article.resourcePath}" title="${article.name}" target="_blank">${article.name}</a></li> #else 
          <li><span class="a">$i</span><a href="${article.resourcePath}" title="${article.name}" target="_blank">${article.name}</a></li> #end #set($i = $i + 1) #end 
         </div>
        </div> 
        <div class="AD-240"> 
         <script type="text/javascript">BAIDU_CLB_SLOT_ID = "1174731";</script> 
         <script type="text/javascript" src="http://cbjs.baidu.com/js/o.js"></script> 
        </div> 
       </div> 
      </div> 
     </div>
     <!-- /right --> 
    </div> 
   </div> 
   <div class="c1 container-bottom"> 
   </div>
  </div> 
   <div class="footer">
  <p class="p_1"> #foreach($firstLevelLink in $page.firstLevelLinks) <a href="${firstLevelLink.resourcePath}" title="${firstLevelLink.name}">${firstLevelLink.name}</a>| #end </p>
  <div class="footer_service">
    <ul>
        <li><a href="$!page.selfMLink" >手机版</a></li>
        <li><a href="$!page.selfLink" >电脑版</a></li>
    </ul>
