  <script type="text/javascript" src="http://www.456fs.com/staticResource/js/456fs.js"></script> 
  <script type="text/javascript">BAIDU_CLB_SLOT_ID = "1174723";</script> 
  <script type="text/javascript" src="http://cbjs.baidu.com/js/o.js"></script> 
  <script type="text/javascript">BAIDU_CLB_SLOT_ID = "1176536";</script> 
  <script type="text/javascript" src="http://cbjs.baidu.com/js/o.js"></script>  
  $!page.countCode
 </body>
